.payPrice{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #555;
  height: 240rpx;
  flex-direction: column;
  border-top: #aaa;
}
.payPrice>text:nth-child(2){
  font-size: 18px;
  margin-top: 10rpx;
  color: #000;
}
.wxPay{
  display: flex;
  flex-direction:column; 
  background-color: #fff;
  padding: 0 20rpx;
  font-size: 14px;
  color: #555;
  width: 100%;
  box-sizing: border-box;
}
.payContainer{
  padding: 35rpx 0;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;  
}

.pay{
  display: flex;
  justify-content: space-between;
}
.pay image{
  width:65rpx;
  height:65rpx;
}
.o-Btn{
  margin-top: 10%;
  width: 90%;
  background-color: #06bf04;
  color: #fff;  
}
.bg-Img{
  position: fixed;
  bottom: 0;left: 0;
  width: 100%;
  height: 70px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center
}
.bg-Img>image{width:110px;height:30px;}


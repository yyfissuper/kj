/* 商品列表*/
page{background-color: #eee}
.pro_nav{
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 40rpx 20rpx;
    border-top: 1rpx solid #aaa;
    position: fixed;
    top: 0%;left: 0%;
    width: 100%;
    z-index: 1;
    font-size: 12px;
    background-color: #fff;
}
.pro_price{display:flex;align-items: center}
.pro_image{width: 60rpx;height: 60rpx;position: relative;}
.noactive{color: #555555; }
.active{color: #ff0000;}
/*产品*/
.pro_parent{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  padding-top: 16%;
}
.pro_list{
    width: 49.3%;
    box-sizing: border-box;
    height: 500rpx;
    margin-bottom: 10rpx;
    padding: 10rpx 0;
    background-color:#fff; 
}
.list_image{
    width:100%;   
    height:333rpx;
    box-sizing: border-box;
}
.pro_detail{
    width: 98%;
    height: 69rpx;
    display: -webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;  
    text-overflow: ellipsis;
    font-size: 12px;
    overflow: hidden;
    margin-top:10rpx; 
    padding: 0 15rpx;
}
.pro_price{
    display: flex;
    justify-content: space-between;
    margin-top:20rpx;
    padding: 0 15rpx;

}
.pro_price text:nth-child(1){
    font-size: 12px;
    color: #ff0000;
}
.pro_price text:nth-child(2){
    font-size: 10px;
    color: #555555;
}


